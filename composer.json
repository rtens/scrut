{
    "name": "rtens/scrut",
    "description": "Light-weight test runner for PHP",
    "keywords": ["test", "test runner", "tdd", "bdd"],
    "homepage": "http://github.com/rtens/scrut",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolas Martens",
            "email": "Nikolas.M@rtens.org",
            "homepage": "http://rtens.org"
        }
    ],
    "require": {
        "php": ">=5.5",
        "watoki/factory": "1.*"
    },
    "bin": ["bin/scrut"],
    "autoload": {
        "psr-4": {
            "rtens\\scrut\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\rtens\\scrut\\": "spec/"
        }
    }
}
